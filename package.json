{
  "name": "uix-ssr-hybrid-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch": "shadow-cljs watch app",
    "compile": "shadow-cljs compile app",
    "release": "shadow-cljs release app",
    "server": "clj -M -m app.server",
    "dev": "npm-run-all --parallel watch server",
    "dev:concurrently": "concurrently \"npm run watch\" \"npm run server\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "npm-run-all": "^4.1.5",
    "shadow-cljs": "^2.26.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
